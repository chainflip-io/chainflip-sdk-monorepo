{
  "name": "@chainflip-io/chainflip-sdk",
  "version": "0.0.32",
  "description": "",
  "exports": {
    "./swap": {
      "import": "./dist/swap.mjs",
      "require": "./dist/swap.js",
      "type": "./dist/swap.d.ts"
    },
    "./swap/types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "type": "./dist/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "swap": [
        "dist/swap.d.ts"
      ],
      "swap/types": [
        "dist/types.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "jest --forceExit",
    "test:ci": "CI=1 jest --forceExit",
    "build": "rm -rf dist && tsup",
    "build:abis": "typechain --target=ethers-v5 ./src/swap/abis/**",
    "start": "node ./dist/cjs/swap/index.js",
    "dev": "tsc-watch --noClear --compiler ./node_modules/typescript/bin/tsc --onFirstSuccess \"nodemon\"",
    "lint": "pnpm eslint --max-warnings 0 './**/*.ts'",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "18.16.0",
    "pnpm": ">=7"
  },
  "repository": "https://github.com/chainflip-io/chainflip-sdk.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@polkadot/util": "^12.0.1",
    "@polkadot/util-crypto": "^12.0.1",
    "axios": "^1.3.4",
    "ethers": "5.7.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^11.0.0",
    "tsup": "^6.7.0",
    "typechain": "^8.1.1"
  }
}
