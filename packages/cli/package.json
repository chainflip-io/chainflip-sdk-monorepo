{
  "name": "@chainflip/cli",
  "version": "1.12.1-wbtc-dev.7",
  "description": "",
  "main": "./dist/lib.mjs",
  "types": "./dist/lib.d.ts",
  "type": "module",
  "bin": {
    "chainflip": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib.d.ts",
        "default": "./dist/lib.mjs"
      },
      "require": {
        "types": "./dist/lib.d.cts",
        "default": "./dist/lib.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/lib.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "prepublish": "pnpm build",
    "build": "rm -rf dist && tsup",
    "test:ci": "exit 0",
    "typecheck": "tsc --noEmit",
    "eslint:check": "pnpm eslint --max-warnings 0 './**/*.ts'",
    "prettier:base": "prettier ./** --ignore-path=../../.prettierignore",
    "prettier:check": "pnpm prettier:base --check",
    "prettier:write": "pnpm prettier:base --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chainflip/bitcoin": "^1.2.7",
    "@chainflip/rpc": "2.0.6-wbtc-dev.4",
    "@chainflip/solana": "^2.2.0",
    "@chainflip/utils": "0.11.2-wbtc-dev.2",
    "ioredis": "^5.6.1",
    "ws": "^8.18.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.33",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "axios": "^1.10.0",
    "ethers": "^6.14.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chainflip-io/chainflip-sdk-monorepo.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
