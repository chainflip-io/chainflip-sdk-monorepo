    "ws": "^8.18.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.33",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "axios": "^1.8.4",
